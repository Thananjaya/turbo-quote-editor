<main class="container">
  <%= turbo_frame_tag "quote_first_frame" do %>
    <div class="header">
      <h1>Quotes</h1>
      <%= link_to "New quote", new_quote_path, data: {turbo_frame: 'quote_second_frame'}, class: "btn btn--primary" %>
    </div>
  <% end %>
  <%= turbo_frame_tag "quote_second_frame" do %>
    <%= render @quotes %>
  <% end %>
</main>